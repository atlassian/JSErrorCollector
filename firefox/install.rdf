<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">

    <em:id>JSErrorCollector@jsourcerer.com</em:id>
    <em:name>JavaScript error collector</em:name>
    <em:version>1.0</em:version>
    <em:description>JavaScript error collector making JS errors available in window's scope.</em:description>
    <em:creator>Marc Guillemot</em:creator>
    <!-- optional items -->
    <em:contributor>Adam Chevalier</em:contributor>
    <em:contributor>Christopher Darroch</em:contributor>
    <em:contributor>Greg Gross</em:contributor>
    <em:contributor>Joakim Recht</em:contributor>
    <em:contributor>Xiao Di Guan</em:contributor>
    <em:homepageURL>http://github.com/atlassian/JSErrorCollector</em:homepageURL>

    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.6</em:minVersion>
        <em:maxVersion>45.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>

  </Description>

</RDF>
